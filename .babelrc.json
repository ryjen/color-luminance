{
  "presets": ["@babel/preset-env", "@babel/preset-flow"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "types": "./src/types",
          "matchers": "./src/matchers",
          "parsers": "./src/parsers"
        }
      }
    ]
  ]
}